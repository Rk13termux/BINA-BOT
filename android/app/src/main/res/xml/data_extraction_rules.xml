<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="file" path="." />
        <include domain="sharedpref" path="." />
        <exclude domain="file" path="no_backup/" />
        <exclude domain="sharedpref" path="secure_prefs.xml" />
    </cloud-backup>
    <device-transfer>
        <include domain="file" path="." />
        <include domain="sharedpref" path="." />
        <exclude domain="file" path="no_backup/" />
        <exclude domain="sharedpref" path="secure_prefs.xml" />
    </device-transfer>
</data-extraction-rules>
