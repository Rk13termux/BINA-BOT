# Invictus Trader Pro - Configuración MEJORADA
# ===============================================
# Versión optimizada con múltiples modelos Groq gratuitos

# Binance API Configuration
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
BINANCE_BASE_URL=https://api.binance.com
BINANCE_TESTNET_URL=https://testnet.binance.vision
BINANCE_WS_URL=wss://stream.binance.com:9443/ws
BINANCE_WS_TESTNET_URL=wss://testnet.binance.vision/ws

# Groq AI Configuration - MODELOS GRATUITOS VERIFICADOS
# ======================================================
GROQ_API_KEY=your_groq_api_key_here
GROQ_BASE_URL=https://api.groq.com/openai/v1/chat/completions

# Modelo Principal (Más Potente - GRATIS)
GROQ_MODEL_PRIMARY=llama-3.3-70b-versatile
GROQ_MAX_TOKENS_PRIMARY=2048
GROQ_TEMPERATURE_PRIMARY=0.2

# Modelo Fallback 1 (Rápido - GRATIS)
GROQ_MODEL_FALLBACK1=llama-3.1-8b-instant
GROQ_MAX_TOKENS_FALLBACK1=1024
GROQ_TEMPERATURE_FALLBACK1=0.3

# Modelo Fallback 2 (Alternativo - GRATIS)
GROQ_MODEL_FALLBACK2=gemma2-9b-it
GROQ_MAX_TOKENS_FALLBACK2=1024
GROQ_TEMPERATURE_FALLBACK2=0.3

# Modelo Experimental (Herramientas Avanzadas - GRATIS)
GROQ_MODEL_EXPERIMENTAL=compound-beta
GROQ_MAX_TOKENS_EXPERIMENTAL=1024
GROQ_TEMPERATURE_EXPERIMENTAL=0.1

# Rate Limits Gratuitos Confirmados
GROQ_REQUESTS_PER_MINUTE=15
GROQ_REQUESTS_PER_DAY=200
GROQ_TOKENS_PER_MINUTE=18000

# Application Configuration
APP_NAME=Invictus Trader Pro
APP_VERSION=1.1.0
DEBUG_MODE=true
LOG_LEVEL=debug
REFRESH_INTERVAL_MS=1000

# AI Features Configuration
ENABLE_AI_ANALYSIS=true
AI_ANALYSIS_INTERVAL_SECONDS=30
AI_ENABLE_MARKET_SENTIMENT=true
AI_ENABLE_TECHNICAL_ANALYSIS=true
AI_ENABLE_NEWS_ANALYSIS=true
AI_ENABLE_PATTERN_RECOGNITION=true

# Trading Configuration
ENABLE_REAL_TRADING=false
ENABLE_PAPER_TRADING=true
ENABLE_WEBSOCKET=true
DEFAULT_POSITION_SIZE=0.01
MAX_DAILY_TRADES=10
RISK_MANAGEMENT_ENABLED=true

# Security Settings
ENCRYPT_STORAGE=true
SESSION_TIMEOUT_MINUTES=60
ENABLE_2FA=false

# Performance Settings
MAX_CANDLES_CACHE=2000
WEBSOCKET_RECONNECT_DELAY_MS=3000
API_REQUEST_TIMEOUT_MS=15000
CACHE_EXPIRE_MINUTES=5

# UI Settings
DEFAULT_THEME=dark
DEFAULT_SYMBOL=BTCUSDT
DEFAULT_TIMEFRAME=1m
CHART_CANDLES_COUNT=100
ENABLE_SOUND_ALERTS=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DARK_MODE=true

# Notification Settings
ENABLE_PRICE_ALERTS=true
ENABLE_AI_SIGNAL_ALERTS=true
ENABLE_NEWS_ALERTS=true
ALERT_SOUND_VOLUME=0.7

# Advanced Features
ENABLE_MULTI_TIMEFRAME_ANALYSIS=true
ENABLE_CORRELATION_ANALYSIS=true
ENABLE_VOLUME_PROFILE=true
ENABLE_ORDER_BOOK_ANALYSIS=true

# Development Settings
MOCK_BINANCE_API=false
LOG_API_REQUESTS=true
ENABLE_PERFORMANCE_MONITORING=true
